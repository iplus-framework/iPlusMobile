    xmlns:local="clr-namespace:gip.vbm.mobile.Views;assembly=gip.vbm.mobile"
    >

    <FlyoutItem Title="About">
        <ShellContent Title="About"
                      ContentTemplate="{DataTemplate local:AboutPage}" />
